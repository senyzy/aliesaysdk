{
    "name": "senyzy/aliesaysdk",
    "description": "It simplifies alipaysdk/easysdk and eliminates unnecessary functions",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "alibabacloud/tea": "^3.1",
        "guzzlehttp/guzzle": ">=6.3",
        "alibabacloud/tea-fileform": "^0.3.2",
        "ext-curl":"*",
        "ext-ctype":"*",
        "ext-dom":"*",
        "ext-fileinfo": "*",
        "ext-json":"*",
        "ext-libxml":"*",
        "ext-simplexml":"*",
        "ext-mbstring":"*",
        "ext-openssl":"*",
        "ext-xmlwriter": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "senyzy",
            "email": "senyzy@foxmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Alipay\\EasySDK\\": "src/"
        }
    }
}
